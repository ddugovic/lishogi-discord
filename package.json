    "discord-api-types": "^0.37.5",
    "discord-lister": "^1.4.0",
    "discord.js": "^14.11.0",
    "emoji-flags": "^1.3.0",
    "friendly-numbers": "^1.0.1",
    "mongoose": "^7.2.0",
    "plural": "^1.1.0",
    "re2": "^1.19.0",
    "reddit-fetch": "^1.5.1",
    "sanitize-html": "^2.10.0",
    "unix-timestamp": "^1.0.2",
    "url-regex-safe": "^3.0.0",
