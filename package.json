    "emoji-flags": "^1.3.0",
    "friendly-numbers": "^1.0.1",
    "html-to-md": "^0.8.0",
    "mongoose": "^6.8.0",
    "ndjson-parse": "^1.0.4",
    "plural": "^1.1.0",
    "quickchart-js": "^3.1.0",
    "re2": "^1.18.0",
